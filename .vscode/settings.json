{
  "python.pythonPath": ".env/bin/python",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    "scraping/**": true,
    ".tmp/**": true,
    ".env/**": true,
    "**/__pycache__/**": true,
  }
}